library(knitr)
library(tidyverse)
knitr::opts_chunk$set(echo = TRUE)
#Data Set of -7,1,2,2,3,4,5,5,6,22
#Type
#Definition of Mean "Measure of Location"
#Definition of Median
#Definition of Mode
Type <- c("Description", "Example", "Result")
Mean <- c("Sum of all of the numbers divided by the number of numbers", "(-7 + 1 + 2 + ... + 22)/10", 4.3)
Median <- c("Value separating the upper and lower halves", "Between 3 and 4; Mean of 3 and 4", 3.5)
Mode <- c("Most frequent value in a dataset", "2 and 5 are equally frequent; bimodal distribution", "2 and 5")
location_measure <- data.frame(Type, Mean, Median, Mode)
kable(location_measure)
Type <- c("Description", "Example", "Result")
Range <- c("Difference between the highest and lowest numbers", "22-(-7)", 29)
Standard_Deviation <- c("Amount of dispersion from the mean", "$2+1$", 3.5)
Interquartile_Range <- c("Most frequent value in a dataset", "Bimodal", "2 and 5")
spread_measure <- data.frame(Type, Range, Standard_Deviation, Interquartile_Range)
kable(spread_measure)
